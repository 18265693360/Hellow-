page{
  width: 100%;
  height: 100%;
  background: #fff;
}
.header{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-bottom: 40rpx;
}
.title-img{
  width: 200rpx;
  height: 136rpx;
}
.container {
  margin-top: 20rpx;
  width: 100%;
}

.input-phone {
  width: 100%;
  height: 90rpx;
  border-bottom: 1rpx solid #d6dbe2;
  margin-left: 25rpx;
  display: flex;
  align-items: center;
}

.input-identify{
  width: 100%;
  height: 90rpx;
  margin-left: 25rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #d6dbe2;
}
.phone-input{
  width: 630rpx;
  font-size: 28rpx;
  padding-left: 10rpx;
}
.user-input {
  width: 450rpx;
  font-size: 28rpx;
  padding-left: 10rpx;
}

.img-icon {
  width: 50rpx;
  height: 60rpx;
}

.get-identify {
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-right: 25rpx;
  font-size: 26rpx;
  color: #4e60df;
  background: #fff;
  border: 1rpx solid #4e60df;
  border-radius: 60rpx;
}

.get-identify-wite{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  margin-right: 25rpx;
  font-size: 26rpx;
  color: #4e60df;
  background: #fff;
  border: 1rpx solid #4e60df;
  border-radius: 60rpx;
}
.btn-next{
  margin-top: 10rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  height: 80rpx;
  background: linear-gradient(to right, #8896ff, #485adc);
  color: #fff;
  line-height: 80rpx;
  font-size: 28rpx;
}

.agreement{
  margin-top: 60rpx;
  font-size: 24rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
  color: #e0e0e0;
}
.agreement-url{
  color: #8896ff;
   /* text-decoration-line: underline; */
   display: inline;
}