page {
  height: 100%;
}

.header {
  width: 100%;
  background: #fff;
}

.function {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.title-img {
  width: 97%;
  height: 384rpx;
  margin-top: 5rpx;
}

.novice-line {
  width: 75%;
  height: 4rpx;
  background: #fff0ed;
}

.novice {
  width: 94%;
  margin-left: 3%;
  margin-top: -60rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.novice-title {
  width: 23%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.novice-text {
  color: #f33;
  font-size: 28rpx;
}

.novice-img {
  width: 35rpx;
  height: 35rpx;
}

.bg-header_foot {
  width: 100%;
  height: 24rpx;
}

.body {
  width: 100%;
  height: 480rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.body-header {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 30rpx;
}

.notice {
  display: flex;
  flex-direction: row;
  margin-left: 50rpx;
}

.notice-img {
  width: 60rpx;
  height: 58rpx;
}

.badge {
  position: absolute;
  margin-top: -6rpx;
  margin-left: 45rpx;
  display: inline-block;
    min-width: 25rpx;
    height: 35rpx;
    line-height: 39rpx;
    padding: 0 6rpx;
    font-size: 22rpx;
    text-align: center;
    font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
    text-rendering: optimizeLegibility;
    -webkit-font-smoothing: antialiased;
    color: #FFF;
    background: #f5342f;
    border-radius: 18rpx;
    overflow: hidden;
}

.login-title {
  font-size: 38rpx;
  margin-left: 100rpx;
}

.icon-avatar {
  width: 130rpx;
  height: 130rpx;
  margin-top: 30rpx;
  border-radius: 50%;
  

}

.avatar-text {
  margin-top: 10rpx;
  font-size: 40rpx;
  font-family: "Helvetica Neue",
                 Helvetica,
                 'microsoft yahei ui',
                 'microsoft yahei',
                 'simhei',
                 Arial,
                 sans-serif;
}

.options {
  margin-top: 10rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.option {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.option-icon {
  width: 90rpx;
}


.option-text {
  margin-top: 10rpx;
  font-size: 26rpx;
}

.btn-login {
  width: 300rpx;
  height: 75rpx;
  border-radius: 75rpx;
  margin-top: 30rpx;
  padding: 0;
  line-height: 75rpx;
  text-align: center;
  background: linear-gradient(to right, #8896ff, #485adc);
  color: white;
  font-size: 28rpx;
}
.btn-userInfo{
  position: absolute;
  z-index: 3;
  opacity: 0;
  width: 130rpx;
  height: 130rpx;
  margin-top: 115rpx;
  border-radius: 50%;
}
.login-text {
  width: 100%;
  margin-top: 40rpx;
  font-size: 34rpx;
  color: black;
  text-align: center;
}

.foot {
  width: 96%;
  height: 60rpx;
  margin-top: 30rpx;
  margin-bottom: 30rpx;
  border-radius: 30rpx;
  background: #fff;
  display: flex;
  border: 1px solid #f0f0f0;
  flex-direction: row;
  align-items: center;
  background-color: #feffef;
}

.horn-img {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
}

.foot-text {
  margin-left: 15rpx;
  font-size: 24rpx;
  color: #686868;
  margin-right: 10rpx;
}
.tubiao{
  width: 18rpx;
  height: 24rpx;
  position: relative;
  top: 3rpx;
  left: 6rpx;
}
.footer-tip{
  height:60rpx;
line-height:60rpx;
background-color:#f5f6ff;
border-radius:30rpx;
font-size:26rpx;
color:#4e5fdd;
width:570rpx;
border:1px solid #e1e0e0;
text-align:center;
margin-top: 20rpx
}



.content{
  background-color: #fff;
  margin-top: 20rpx;
  padding: 0 15rpx;
  font-size: 28rpx;
}
.content-top{
  line-height: 90rpx;
  border-bottom: 1px solid #f0f0f0;
}
.content-top text{
  font-size: 36rpx;
  color: #000;
  font-weight: 600;
}
.flex{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.content-item{
  background-color: #f4f6fa;
  width: 49%;
  height: 135rpx;
  border-radius: 10rpx;
  padding: 25rpx;
  box-sizing: border-box;
  margin: 25rpx 0;
}
.content-item view{
  overflow: hidden;
  /* word-break: break-all; */
  text-overflow: ellipsis;
  flex: 1;
  display: -webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient: vertical;
  line-height: 42.5rpx;
}
.img-title{
  position:absolute;
top:312rpx;
font-size:36rpx;
font-weight:600;

}
.abnor-hide {
  display: none;
}
.rad{
  width: 90rpx;
  height: 90rpx;
  border-radius: 45rpx;
  background-color: #fff;
}